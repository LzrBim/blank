{% extends 'front/layout.twig' %}

{% block content %}

	<div class="galleryWrapper container">

    <div class="row">
  
      <div class="col-sm-12">
      
      	<h1>{{title}}</h1>

        <ul>
        {% for gallery in galleries %}
          <li><a href="/gallery/{{gallery.permalink}}">{{ gallery.title }}</a></li>
        {% endfor %}
        </ul>
      
      </div><!-- /.col -->
      
    </div><!-- /.row -->
    
  </div>

	
  
{% endblock %}